<ul class="breadcrumb">	<li><a href="/">Home</a></li>  	<li><a href="/school/">Schools</a></li>  	<li class="active">Add School</li></ul><fieldset>	<legend><i class="icon-plus"></i> Add School</legend>	<div class="row-fluid">		<div class="col-lg-12"><?php echo $this->form;?></div>	</div></fieldset><script>$(function(){	if($("#district").val() != '')		getByDistrict($("#district").val());});function getByDistrict(district){	$.ajax({		url: '/sub-division/fetch-by-district/district/'+district	})	.done(function(result){		var current = $('#sub_division').val();		var opts = '';		$(result).each(function(k,data){			opts += '<option value="'+data.name+'">'+data.name+'</option>';		});		$('#sub_division').html(opts);		$('#sub_division').val(current);	});}</script>